{
    "name": "shibuyakosuke/laravel-db-schema",
    "description": "laravel-db-schema",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ShibuyaKosuke",
            "email": "shibuya@imagenious.co.jp"
        }
    ],
    "autoload": {
        "psr-4": {
            "ShibuyaKosuke\\LaravelDbSchema\\": "src/",
            "ShibuyaKosuke\\LaravelDbSchema\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShibuyaKosuke\\LaravelDbSchema\\Providers\\ServiceProvider"
            ],
            "aliases" : {
                "Table": "ShibuyaKosuke\\LaravelDbSchema\\Facades\\Table",
                "Column": "ShibuyaKosuke\\LaravelDbSchema\\Facades\\Column"
            }
        }
    },
    "require": {
        "php": "^7.2.5 | ^7.3 | ^7.4",
        "diplodocker/comments-loader": "^0.0.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^8.0 | ^9.0",
        "mockery/mockery": "^1.3 | ^1.4",
        "orchestra/testbench": "^5.0 | ^6.0"
    }
}
